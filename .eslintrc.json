
 {
  "parser": "@typescript-eslint/parser",
  "extends": "eslint:recommended",
  "env": {
    "node": true,
    "browser": true,
    "webextensions": true
  },
  "plugins": ["@typescript-eslint"],
  "rules": {
    "no-console": "error",
    // https://typescript-eslint.io/rules/no-unused-vars/
    "no-unused-vars": "off",
    "@typescript-eslint/no-unused-vars": ["error", { "argsIgnorePattern": "^_"}]
  }
}
